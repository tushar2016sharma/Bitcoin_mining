variables <- c('power.GUESS..GW', 'annualised.consumption.GUESS..TWh', 'Estimated.efficiency..J.Th',
'Hydro.only..MtCO2e', 'Estimated..MtCO2e', 'Coal.only..MtCO2e',
'Emission.intensity..gCO2e.kWh', 'Hash.rate.MH.s')
# Generate histograms for each variable
par(mfrow=c(4, 2))  # Set up a 4x2 grid for plots
for(var in variables) {
var_sym <- sym(var)
ggplot(Cleaned_bitcoin_mining, aes(x = !!var_sym)) +
geom_histogram(fill='skyblue', bins=30) +
geom_density(aes(y=..count..), color="red") +  # Overlay density plot
theme_minimal() +
labs(title=paste('Histogram of', var), x=var, y='Frequency')
}
